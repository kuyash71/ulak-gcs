{
  "schema_version": "1.0.0",
  "profile_id": "safe",
  "display_name": "Safe",
  "protected": false,
  "constraints": {
    "panic_override_allowed": false
  },
  "policy": {
    "severity_levels": [
      "WARN",
      "ERROR",
      "CRITICAL"
    ],
    "defaults": {
      "unknown_event": {
        "severity": "CRITICAL",
        "action": "PANIC_RTL"
      }
    },
    "mappings": [
      {
        "event_code": "TELEMETRY_LOSS",
        "severity": "CRITICAL",
        "action": "PANIC_RTL"
      },
      {
        "event_code": "CC_LINK_LOSS",
        "severity": "CRITICAL",
        "action": "PANIC_RTL"
      },
      {
        "event_code": "STREAM_LOSS",
        "severity": "ERROR",
        "action": "REQUEST_CONFIRMATION",
        "timeout_action": "PANIC_RTL"
      },
      {
        "event_code": "VISION_LOST",
        "severity": "ERROR",
        "action": "REQUEST_CONFIRMATION",
        "timeout_action": "PANIC_RTL"
      },
      {
        "event_code": "LOW_BATTERY",
        "severity": "CRITICAL",
        "action": "PANIC_RTL"
      },
      {
        "event_code": "INVALID_OPERATOR_ACTION",
        "severity": "ERROR",
        "action": "REJECT_COMMAND"
      }
    ]
  },
  "timers": {
    "error_confirm_window_sec": 3
  },
  "limits": {
    "command_rate_limit_per_sec": 3
  }
}
